<body>
    <div class="container">
        <h3>Edit the data</h3>
        <br>
         <form action="/updateJobs" method="POST">
            <div class="form-group">
                <label>Job Id</label>
                <input type="text" name="jobId" value="<%= job.id %>">
            </div>
            <div class="form-group">
                <label>Part Id</label>
                <input type="text"  name="partId" value="<%= job.partId %>">
            </div>
            <div class="form-group">
                <label>Quantity</label>
                <input type="text"  name="qty" value="<%= job.qty %>">
            </div>
            <button class="btn btn-primary">Submit</button>
        </form>
        <br>
        <form action="/">
            <button type= "submit" class="btn btn-secondary">Home</button>
        </form>
</div>  
</body>