<body>
    <div class="container">
        <form action="/">
            <button type= "submit" class="btn btn-info">Go to main page</button>
        </form>
        <br>
        <h2>Part No. <div style="color: red; display: inline-block;"><%= part.partId%></div></h2>
        <br><br>
        <h5>Edit Part <%= part.partId%> details</h5>
        <br>
        <row>
            <col md="4">
            <form action="/editPart/<%= part.partId%>" method="POST">
                <div class="form-group">
                    <label for="partName">Part Name</label>
                    <input type="text" required value="<%= part.partName %>" class="form-control" name="partName" placeholder="Please enter Name of the part.">
                </div>
                <div class="form-group">
                    <label for="qoh">Quantity on hand (Available quantity) </label>
                    <input type="text" class="form-control" value="<%= part.qoh %>" name="qoh"  placeholder="Please enter Quantity on Hand value.">
                </div>
                <button type="submit" class="btn btn-success">Edit Part</button>
            </form>
            </col>
        </row>
        <br>
</div>  
</body>